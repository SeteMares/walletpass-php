{
    "name": "setemares/walletpass-php",
    "description": "PHP client for WalletPass.jp",
    "keywords": [
        "openapi",
        "php",
        "sdk",
        "api",
        "walletpasss"
    ],
    "homepage": "https://walletpass.jp",
    "license": "proprietary",
    "authors": [
        {
            "name": "tectiv3",
            "homepage": "https://github.com/tectiv3"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "WalletPassJP\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WalletPassJP\\ServiceProvider"
            ]
        }
    }
}